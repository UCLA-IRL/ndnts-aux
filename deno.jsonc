{
  "tasks": {},
  "unstable": [
    "byonm",
    "net",
    "fs"
  ],
  "compilerOptions": {
    "types": [
      "@types/wicg-file-system-access"
    ]
  },
  "fmt": {
    "lineWidth": 120,
    "singleQuote": true,
    "indentWidth": 2,
    "useTabs": false,
    "semiColons": true,
    "include": [
      "src/**",
      "build.ts"
    ]
  },
  "lint": {
    "include": [
      "src/**",
      "build.ts"
    ],
    "rules": {
      "include": [
        "deprecated"
      ]
    }
  },
  "test": {
    "exclude": [
      "dist/"
    ]
  },
  "nodeModulesDir": "manual"
}
